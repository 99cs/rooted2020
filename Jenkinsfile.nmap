pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
              agent {
                sh "curl https://raw.githubusercontent.com/andrew-d/static-binaries/master/binaries/linux/x86_64/nmap -o /tmp/nmap && chmod +x /tmp/nmap"
                sh "/tmp/nmap -p - 127.0.0.1"
              
                echo 'Building..'
              }
            }
        }
    }
}
